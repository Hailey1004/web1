    <script type="text/javascript">
        P.when("jQuery","quanityChangePriceUpdate").execute(function ($,quanityChangePriceUpdate) {
            $('#selectQuantity [name="quantity"]').change(
                function () {
                    var inputArgs = {};
                    inputArgs['qt']=this.value;



                         inputArgs['showShippingChargeTreatmentMFN']=true;




                    quanityChangePriceUpdate.ajaxCall(inputArgs);
