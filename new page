<script type="text/javascript">

function prefetchTYPAssets() {
    var imageAssets = new Array();
    var jsCssAssets = new Array();
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/39/x-locale/common/buttons/continue-shopping._CB278388326_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/39/x-locale/communities/social/snwicons_v2._CB278393011_.png");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/39/checkout/assets/carrot._CB278266040_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/39/checkout/thank-you-page/assets/yellow-rounded-corner-sprite._CB278388569_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/39/gno/sprites/nav-sprite-global_bluebeacon-1x_optimized._CB453809735_.png");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/39/x-locale/common/transparent-pixel._CB279869683_.gif");

    // pre-fetching image assets
    for (var i=0; i<imageAssets.length; i++) {
       new Image().src = imageAssets[i];
    }
    // pre-fetching css and js assets based on different browser types
    var isIE = /*@cc_on!@*/0;
    var isFireFox = /Firefox/.test(navigator.userAgent);
    if (isIE) {
      for (var i=0; i<jsCssAssets.length; i++) {
        new Image().src = jsCssAssets[i];
      }
    }
    else if (isFireFox) {
      for (var i=0; i<jsCssAssets.length; i++) {
        var o =  document.createElement("object");
        o.data = jsCssAssets[i];
        o.width = o.height = 0;
        document.body.appendChild(o);
      }
    }
}

    var onload = function () {
        setTimeout(prefetchTYPAssets, 2000);
    };
    if (window.addEventListener) {
        window.addEventListener("load", onload);
    } else if (window.attachEvent) { /* for <= IE 8 */
        window.attachEvent("onload", onload);
    }
</script>

